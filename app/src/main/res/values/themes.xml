<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BottomDynamic" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="BottomNavigationIconStyle" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:background">?android:colorBackground</item>
        <item name="android:selectableItemBackground">@drawable/rounded_background</item>
    </style>

    <style name="BottomNavigationIconStyle2" parent="Widget.MaterialComponents.BottomNavigationView.Colored">

    </style>
</resources>